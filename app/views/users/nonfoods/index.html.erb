 <div>
    <h2>Nonfoods Idea</h2>
    <table>
      <thead>
        <tr>
          <th>投稿画像</th>
          <th>アイデア名</th>
          <th>投稿者</th>
          <th>投稿日時</th>
          <th>いいね</th>
        </tr>
      </thead>
      <tbody>
        <% @nonfoods.each do |nonfood|%>
          <tr>
            <td><%= link_to users_nonfood_path(nonfood) do %>
            	  <%= attachment_image_tag nonfood, :nonfood_image, size: '60x60',　format: 'jpeg' %>
            	<% end %></td>
            <td><%= link_to users_nonfood_path(nonfood) do %>
            	  <%= nonfood.nonfood_name %>
            	<% end %></td>
            <td></td>
          	<td><%= nonfood.created_at.strftime('%Y/%m/%d') %></td>
          	<td></td>
         </tr>
      　<% end %>
      </tbody>
    </table>
  </div>