 <div class="container">
  <div class="low">
    <h1>食品アイデア</h1>
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>アイデア画像/アイデア名</th>
          <th>投稿者名</th>
          <th>投稿日時</th>
          <th>いいね</th>
        </tr>
      </thead>
      <tbody>
        <% @foods.each do |food|%>
          <tr class="food_<%= food.id %>">
            <%= render 'food', food: food %>
         </tr>
      　<% end %>
      </tbody>
    </table>
  <span style="display: block; text-align: center;"><%= paginate @foods %></span>
  </div>
</div>
